{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "rootDir": ".",
  },
  "exclude": ["/node_modules", "/node_modules/*", "/node_modules/**/*"],
  "include": ["./src/**/*"]
}

